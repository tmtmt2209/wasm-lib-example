{
  "name": "wasm-lib-example",
  "version": "1.2.0",
  "description": "Example of library package using wasm-bindgen",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "cargo +nightly build --release --target wasm32-unknown-unknown && wasm-bindgen target/wasm32-unknown-unknown/release/wasm_lib_example.wasm --out-dir lib",
    "test": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tmtmt2209/wasm-lib-example.git"
  },
  "author": "tmtmt2209",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tmtmt2209/wasm-lib-example/issues"
  },
  "homepage": "https://github.com/tmtmt2209/wasm-lib-example#readme",
  "devDependencies": {
    "@types/jasmine": "^2.8.8",
    "eslint": "^5.6.0",
    "flowgen": "^1.3.0",
    "jasmine-core": "^3.2.1",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^2.0.5",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.2",
    "karma-webpack": "^3.0.5",
    "ts-loader": "^4.5.0",
    "typescript": "^3.0.3",
    "typescript-eslint-parser": "^17.0.1",
    "webpack": "^4.19.1"
  }
}
